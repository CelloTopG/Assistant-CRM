{
 "doctype": "DocType",
 "name": "Branch Performance Report",
 "module": "Assistant CRM",
 "custom": 0,
 "is_submittable": 0,
 "autoname": "BRPERF-.YYYY.-.#####",
 "allow_rename": 0,
 "editable_grid": 0,
 "track_changes": 1,
 "field_order": [
  "title",
  "period_type",
  "date_from",
  "date_to",
  "section_filters",
  "region_filter",
  "branch_filter",
  "channel_filter",
  "priority_filter",
  "section_kpis",
  "total_branches",
  "total_issues",
  "total_claims",
  "total_complaints",
  "total_escalations",
  "overall_sla_compliance_percent",
  "avg_issue_resolution_days",
  "avg_claim_resolution_days",
  "avg_complaint_resolution_days",
  "section_visuals",
  "branch_overview_chart_json",
  "sla_branch_chart_json",
  "regional_comparison_chart_json",
  "trend_chart_json",
  "report_html",
  "rows_json",
  "filters_json",
  "generated_at",
  "generated_by"
 ],
 "fields": [
  {"fieldname": "title", "label": "Title", "fieldtype": "Data"},
  {"fieldname": "period_type", "label": "Period Type", "fieldtype": "Select", "options": "Monthly\nQuarterly\nCustom", "default": "Monthly", "reqd": 1},
  {"fieldname": "date_from", "label": "Date From", "fieldtype": "Date", "reqd": 1},
  {"fieldname": "date_to", "label": "Date To", "fieldtype": "Date", "reqd": 1},

  {"fieldname": "section_filters", "fieldtype": "Section Break", "label": "Filters"},
  {"fieldname": "region_filter", "label": "Region", "fieldtype": "Select", "options": "All\nLusaka\nCopperbelt\nNorthern\nEastern\nSouthern\nWestern\nCentral\nLuapula\nMuchinga\nNorth-Western", "default": "All"},
  {"fieldname": "branch_filter", "label": "Branch (contains)", "fieldtype": "Data"},
  {"fieldname": "channel_filter", "label": "Channel", "fieldtype": "Select", "options": "All\nWhatsApp\nFacebook\nInstagram\nEmail\nChat\nTelegram\nLinkedIn\nTwitter", "default": "All"},
  {"fieldname": "priority_filter", "label": "Priority", "fieldtype": "Select", "options": "All\nLow\nMedium\nHigh\nUrgent", "default": "All"},

  {"fieldname": "section_kpis", "fieldtype": "Section Break", "label": "KPI Summary"},
  {"fieldname": "total_branches", "label": "Branches", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "total_issues", "label": "Issues (Total)", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "total_claims", "label": "Claims (Total)", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "total_complaints", "label": "Complaints (Total)", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "total_escalations", "label": "Escalations (Total)", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "overall_sla_compliance_percent", "label": "Overall SLA %", "fieldtype": "Float", "precision": 2, "read_only": 1},
  {"fieldname": "avg_issue_resolution_days", "label": "Avg Issue Resolution (days)", "fieldtype": "Float", "precision": 2, "read_only": 1},
  {"fieldname": "avg_claim_resolution_days", "label": "Avg Claim Resolution (days)", "fieldtype": "Float", "precision": 2, "read_only": 1},
  {"fieldname": "avg_complaint_resolution_days", "label": "Avg Complaint Resolution (days)", "fieldtype": "Float", "precision": 2, "read_only": 1},

  {"fieldname": "section_visuals", "fieldtype": "Section Break", "label": "Visualizations"},
  {"fieldname": "branch_overview_chart_json", "label": "Branch Overview Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "sla_branch_chart_json", "label": "SLA by Branch Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "regional_comparison_chart_json", "label": "Regional Comparison Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "trend_chart_json", "label": "Trend Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "report_html", "label": "Report Dashboard", "fieldtype": "HTML"},
  {"fieldname": "rows_json", "label": "Rows (JSON)", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "filters_json", "label": "Filters (JSON)", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "generated_at", "label": "Generated At", "fieldtype": "Datetime", "read_only": 1},
  {"fieldname": "generated_by", "label": "Generated By", "fieldtype": "Link", "options": "User", "read_only": 1}
 ],
 "permissions": [
  {"role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 1},
  {"role": "CRM Administrator", "read": 1, "write": 1, "create": 1},
  {"role": "Senior Management", "read": 1, "write": 1, "create": 1},
  {"role": "Branch Manager", "read": 1, "write": 1, "create": 1}
 ]
}

