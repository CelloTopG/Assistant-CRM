{
 "doctype": "DocType",
 "name": "SLA Compliance Report",
 "module": "Assistant CRM",
 "custom": 0,
 "is_submittable": 0,
 "autoname": "SLACOMP-.YYYY.-.#####",
 "allow_rename": 0,
 "editable_grid": 0,
 "track_changes": 1,
 "field_order": [
  "title",
  "period_type",
  "date_from",
  "date_to",
  "section_filters",
  "branch_filter",
  "role_filter",
  "channel_filter",
  "priority_filter",
  "section_kpis",
  "total_items",
  "within_sla_count",
  "breached_sla_count",
  "compliance_percent",
  "frt_avg_minutes",
  "frt_p90",
  "frt_within_sla_percent",
  "rt_avg_hours",
  "rt_p90",
  "rt_within_sla_percent",
  "escalations_total",
  "escalations_within_sla",
  "escalations_breached",
  "ai_first_responses",
  "section_visuals",
  "chart_overview_json",
  "branch_breakdown_chart_json",
  "role_breakdown_chart_json",
  "trend_chart_json",
  "report_html",
  "rows_json",
  "filters_json",
  "generated_at",
  "generated_by"
 ],
 "fields": [
  {"fieldname": "title", "label": "Title", "fieldtype": "Data"},
  {"fieldname": "period_type", "label": "Period Type", "fieldtype": "Select", "options": "Monthly\nCustom", "default": "Monthly", "reqd": 1},
  {"fieldname": "date_from", "label": "Date From", "fieldtype": "Date", "reqd": 1},
  {"fieldname": "date_to", "label": "Date To", "fieldtype": "Date", "reqd": 1},

  {"fieldname": "section_filters", "fieldtype": "Section Break", "label": "Filters"},
  {"fieldname": "branch_filter", "label": "Branch (contains)", "fieldtype": "Data"},
  {"fieldname": "role_filter", "label": "Role (contains)", "fieldtype": "Data"},
  {"fieldname": "channel_filter", "label": "Channel", "fieldtype": "Select", "options": "All\nWhatsApp\nFacebook\nInstagram\nEmail\nChat\nTelegram\nLinkedIn\nTwitter", "default": "All"},
  {"fieldname": "priority_filter", "label": "Priority", "fieldtype": "Select", "options": "All\nLow\nMedium\nHigh\nUrgent", "default": "All"},

  {"fieldname": "section_kpis", "fieldtype": "Section Break", "label": "KPI Summary"},
  {"fieldname": "total_items", "label": "Items Considered", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "within_sla_count", "label": "Within SLA", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "breached_sla_count", "label": "Breached SLA", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "compliance_percent", "label": "Compliance %", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "frt_avg_minutes", "label": "Avg First Response (min)", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "frt_p90", "label": "P90 First Response (min)", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "frt_within_sla_percent", "label": "FRT within SLA %", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "rt_avg_hours", "label": "Avg Resolution (hrs)", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "rt_p90", "label": "P90 Resolution (hrs)", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "rt_within_sla_percent", "label": "RT within SLA %", "fieldtype": "Float", "read_only": 1, "precision": 2},
  {"fieldname": "escalations_total", "label": "Total Escalations", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "escalations_within_sla", "label": "Escalations within SLA", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "escalations_breached", "label": "Escalations breached SLA", "fieldtype": "Int", "read_only": 1},
  {"fieldname": "ai_first_responses", "label": "AI as First Response (count)", "fieldtype": "Int", "read_only": 1},

  {"fieldname": "section_visuals", "fieldtype": "Section Break", "label": "Visualizations"},
  {"fieldname": "chart_overview_json", "label": "Overview Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "branch_breakdown_chart_json", "label": "Branch Breakdown Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "role_breakdown_chart_json", "label": "Role Breakdown Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "trend_chart_json", "label": "Trend Chart Data", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "report_html", "label": "Report Dashboard", "fieldtype": "HTML"},
  {"fieldname": "rows_json", "label": "Rows (JSON)", "fieldtype": "Code", "read_only": 1},
  {"fieldname": "filters_json", "label": "Filters", "fieldtype": "Code"},
  {"fieldname": "generated_at", "label": "Generated At", "fieldtype": "Datetime", "read_only": 1},
  {"fieldname": "generated_by", "label": "Generated By", "fieldtype": "Link", "options": "User", "read_only": 1}
 ],
 "permissions": [
  {"role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 1},
  {"role": "Customer Service", "read": 1, "write": 1, "create": 1},
  {"role": "Corporate Affairs", "read": 1, "write": 1, "create": 1}
 ]
}

